{
	"defaultEnv": {
		"ENV": "NODE_ENV"
	},
	"sql": {
		"driver": "mysql",
		"host": {
			"ENV": "MYSQL_HOST"
		},
		"database": {
			"ENV": "MYSQL_DATABASE"
		},
		"port": {
			"ENV": "MYSQL_PORT"
		},
		"user": {
			"ENV": "MYSQL_ROOT_USER"
		},
		"password": {
			"ENV": "MYSQL_ROOT_PASSWORD"
		},
		"multipleStatements": true
	}
}